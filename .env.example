# Environment
NODE_ENV=development

# App
PORT=3000

# Database
DATABASE_URL=postgresql://pg:pg@postgres:5432/nflow-chat-assistant
PGUSER=pg
PGPASSWORD=pg
PGDATABASE=nflow-chat-assistant
PGPORT=5432
PGHOST=postgres

# Redis
REDIS_URL=redis://:redis@redis:6379
REDIS_PASSWORD=redis
REDIS_HOST=redis
REDIS_PORT=6379

# Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=7d

# OpenAI Configuration
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL=gpt-4.1
OPENAI_MAX_TOKENS=10000
OPENAI_TEMPERATURE=0.2

# Nflow Configuration
NFLOW_CLIENT_URL=your-nflow-client-url
NFLOW_API_URL=your-nflow-api-url
NFLOW_API_KEY=your-nflow-api-key

# Keycloak Configuration
KEYCLOAK_CLIENT_ID=your-client-id
KEYCLOAK_CLIENT_SECRET=your-client-secret
KEYCLOAK_REALM=your-realm
KEYCLOAK_AUTH_SERVER_URL=https://keycloak.example.com/auth
KEYCLOAK_CALLBACK_URL=http://localhost:3000/auth/keycloak/callback
SESSION_SECRET=your-session-secret-min-32-chars-long
REFRESH_TOKEN_ENCRYPTION_KEY=your-refresh-token-encryption-key
FRONTEND_URL=http://localhost:8501
