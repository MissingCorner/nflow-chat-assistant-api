# Environment
NODE_ENV=development

# App
PORT=3000

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/your-database-name

# Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=7d

# Application
APP_PORT=3000
NODE_ENV=development

# Redis
