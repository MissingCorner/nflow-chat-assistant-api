# Environment
NODE_ENV=development

# App
PORT=3000

# Database
DATABASE_URL=postgresql://pg:pg@localhost:5432/nflow-chat-assistant
PGUSER=pg
PGPASSWORD=pg
PGDATABASE=nflow-chat-assistant
PGPORT=5432
PGHOST=localhost

# Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=7d

# Redis
REDIS_URL=your-redis-url

# OpenAI Configuration
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL=gpt-4.1
OPENAI_MAX_TOKENS=10000
OPENAI_TEMPERATURE=0.2

# Nflow Configuration
NFLOW_CLIENT_URL=your-nflow-client-url
NFLOW_API_URL=your-nflow-api-url
NFLOW_API_KEY=your-nflow-api-key
