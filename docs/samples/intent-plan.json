{
  "summary": "Build a simple personal financial application",
  "tasks": [
    {
      "agent": "ApplicationAgent",
      "description": "Create the personal finance app",
      "data": {
        "action": "create",
        "name": "Personal Finance",
        "description": "Track expenses and income"
      }
    },
    {
      "agent": "ObjectAgent",
      "description": "Create objects to track income and expenses",
      "data": {
        "action": "create",
        "objects": ["Income", "Expense"]
      }
    },
    {
      "agent": "LayoutAgent",
      "description": "Create pages to add and view transactions",
      "data": {
        "action": "create",
        "pages": ["Add Expense", "Transaction List"],
        "bindings": {
          "Expense": "Transaction List"
        }
      }
    },
    {
      "agent": "FlowAgent",
      "description": "Trigger when a transaction is added to update balance",
      "data": {
        "action": "create",
        "trigger": "Transaction added",
        "actionLogic": "Update balance"
      }
    }
  ]
}
